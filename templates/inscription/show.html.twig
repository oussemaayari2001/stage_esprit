                </li>
                 <li class="list-group-item " style="background-color: #fefaf0 !important; border-radius:10px;">
                <a href="{{ path('app_inscription_edit', {'id': espInscription.id}) }}" class="btn btn-secondary">Modifier</a>
                </li>
                <li class="list-group-item " style="background-color: #fefaf0 !important; border-radius:10px;">
                 <a href="{{ path('app_inscription_index') }}" class="btn btn-secondary">Retour</a>
                </li>
                <li class="list-group-item " style="background-color: #fefaf0 !important; border-radius:10px;">
                  
    {{ include('inscription/_delete_form.html.twig') }}
                </li> 
            </ul>

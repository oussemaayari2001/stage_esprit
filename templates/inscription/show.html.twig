            </tr>
            <tr>
                <th>Dern utilisateur</th>
                 <td>{{ espInscription.getDernUtilisateur() }}</td>
            </tr>
            <tr>
                <th>Date preinscription</th>
                <td>{{ espInscription.getDatePreinsc() ? espInscription.getDatePreinsc()|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Date inscription</th>
                <td>{{ espInscription.getDateInsc() ? espInscription.getDateInsc()|date('Y-m-d') : '' }}</td>
            </tr>

             <tr>
                <th>Code cl1</th>
                    <td>{{ espInscription.getCodeCl1() }}</td>
